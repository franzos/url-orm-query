{
  "name": "url-orm-query",
  "version": "0.2.0",
  "description": "End-to-end URL to TypeORM query builder",
  "author": "Franz Geffke <mail@gofranz.com>",
  "repository": "https://github.com/franzos/url-orm-query",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && node build.js && tsc -p tsconfig.json --emitDeclarationOnly",
    "test": "jest --config ./tests/jest.config.ts --forceExit",
    "test:watch": "jest --watchAll --config ./tests/jest.config.ts",
    "typeorm": "./node_modules/.bin/typeorm-ts-node-esm -d tests/utils/ormconfig.ts"
  },
  "keywords": [
    "typeorm",
    "orm",
    "querybuilder"
  ],
  "devDependencies": {
    "typeorm": "^0.3.25",
    "@types/jest": "^29",
    "@types/node": "^22",
    "esbuild": "^0.17.19",
    "esbuild-node-externals": "^1.18.0",
    "jest": "^29",
    "pg": "^8.16.3",
    "rimraf": "^6",
    "ts-jest": "^29.4.0",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.mjs.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/*"
  ]
}
