{
  "name": "url-orm-query",
  "version": "0.0.12",
  "description": "End-to-end URL to TypeORM query builder",
  "author": "Franz Geffke <franz@pantherx.org>",
  "repository": "https://git.pantherx.org/development/libraries/url-orm-query",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && node build.js && tsc --emitDeclarationOnly",
    "test": "jest --config ./tests/jest.config.ts",
    "test:watch": "jest --watchAll --config ./tests/jest.config.ts",
    "typeorm": "./node_modules/.bin/typeorm-ts-node-esm -d tests/utils/ormconfig.ts"
  },
  "keywords": [
    "typeorm",
    "orm",
    "querybuilder"
  ],
  "dependencies": {
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "esbuild": "^0.17.11",
    "esbuild-node-externals": "^1.6.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "jest": "^29.4.3",
    "pg": "^8.9.0",
    "rimraf": "^4.2.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/*"
  ]
}
