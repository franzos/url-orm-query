{
  "name": "url-orm-query",
  "version": "0.0.2",
  "description": "End-to-end URL to TypeORM query builder",
  "author": "Franz Geffke <franz@pantherx.org>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && tsc",
