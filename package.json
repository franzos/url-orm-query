  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5",
    "rimraf": "^4.2.0"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/*"
